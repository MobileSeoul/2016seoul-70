<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <!--
        Base application theme, dependent on API level. This theme is replaced
        by AppBaseTheme from res/values-vXX/styles.xml on newer devices.

    -->


    <!-- Application theme. -->
    <style name="AppTheme" parent="TrueActionBar">
       
        <item name="android:actionBarStyle">@style/AppBaseTheme</item>
        
        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
    </style>
 <style parent="android:Theme.Light" name="AppBaseTheme">
     
     

<!-- Theme customizations available in newer API levels can go in res/values-vXX/styles.xml, while customizations related to backward-compatibility can go here. -->


</style>


    <style name="TrueActionBar" parent="@android:style/Widget.Holo.Light.ActionBar.Solid.Inverse">
        <item name="android:background">#eca8b2</item>
    </style>
    

</resources>
